# libraries needed
library(tidyverse)
library(car)

#  read dataset
bmi_data <- read_csv("dir/ehresp_2014.csv") %>%
  filter(erbmi > 0 )

# fit a glm model 
model <- glm(erbmi ~ euexfreq + euwgt + euhgt + ertpreat, data = bmi_data, family = "gaussian")

# output plots in a 2 x 2 grid 
par(mfrow= c(2,2))
plot(model)

# examine our model
summary(model)

# added-variable plots for the model
avPlots(model)
